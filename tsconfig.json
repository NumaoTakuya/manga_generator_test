{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/pages/index.tsx",
    "src/components/Bubble.tsx",
    "src/components/Tail.tsx",
    "src/pages/panel.tsx",
    "src/utils/hooks/useDetectFace.ts",
    "src/utils/classes/CenteredRect.ts",
    "src/utils/useMouseCenteredRect.js"
, "src/components/CropImage.tsx", "src/components/Layer.tsx", "src/components/NavigationBar.tsx", "src/components/OnomatopoeiaText.tsx", "src/components/Bubbles/tailReducer.ts", "src/utils/hooks/useMouthPosition.ts"  ],
  "exclude": ["node_modules"]
}
